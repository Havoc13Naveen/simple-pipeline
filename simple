pipeline{
    agent any
    tools {
  maven 'maven'
}
    stages{
        stage('build'){
            agent {
                label 'slave_1'
            }
            steps{
                git url: "https://github.com/Havoc13Naveen/calculator.git"
                stash 'source'
            }
        }
        stage('deploy'){
            agent {
                label 'slave_2'
            }
            steps{
                unstash 'source'
              sh  "mvn clean install"
            }
        }
    }
}
